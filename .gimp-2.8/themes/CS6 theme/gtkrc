style "gimp-dockable-style" = "gimp-default-style"
{

    bg[NORMAL]        = "#3c3c3c" # BG for tool-option panel
    bg[PRELIGHT]      = "#3c3c3c" # open-close check-box in opt-panel
    bg[SELECTED]      = "#585858" # Dark-Grey bg color for value bar, like opacity
    bg[INSENSITIVE]   = "#000000" # BlackText in value bar,like opacity
    bg[ACTIVE]        = "#ffd800" # Unknow, Yellow

    fg[NORMAL]        = "#777777" # Font color in panel-bar,like layer mode
    fg[PRELIGHT]      = "#b7b7b7" # Grey Text in Selected value bar,like opacity
    fg[SELECTED]      = "#b7b7b7" # ^^^
    fg[INSENSITIVE]   = "#000000" # Black Text in panel-bar, in no-open-file mode
    fg[ACTIVE]        = "#b7b7b7" # Clicked element - like menu-arrow in tool-panel
                
    text[NORMAL]      = "#5c5c5c" # Grey Outside Border for value bar, value-number
    text[PRELIGHT]    = "#b7b7b7" # Unknow, White
    text[SELECTED]    = "#b7b7b7" # Clicked layer text
    text[INSENSITIVE] = "#000000" # Black unselectable text
    text[ACTIVE]      = "#b7b7b7" # Clicked layer text 

    base[NORMAL]      = "#3c3c3c" # BG color for arrow-bar,layer-mode name
    base[PRELIGHT]    = "#3c3c3c" # Unknow
    base[SELECTED]    = "#585858" # Selected layer bg # Use 4b5367 for dark-blue
    base[INSENSITIVE] = "#3c3c3c" #
    base[ACTIVE]      = "#585858" # Selected layer bg # Use 4b5367 for dark-blue         
        
  GimpFrame::label-bold = 0
  GtkButton::inner-border = {0,0,0,0} # (L,R,T,B)
  GtkButton::focus-line_width = 1
  GtkButton::focus-padding = 0

}

