        bg[NORMAL]        = "#3c3c3c" # Main Window BG and border for Tool/Status/Panel-Bar
        bg[PRELIGHT]      = "#3c3c3c" #
        bg[SELECTED]      = "#585858" # Dark-Grey / Headers in preferences, tool panel (value bar)
        bg[INSENSITIVE]   = "#3c3c3c"
        bg[ACTIVE]        = "#3c3c3c"

        fg[NORMAL]        = "#b7b7b7" # Light-Grey Basic text color
        fg[PRELIGHT]      = "#b7b7b7" # White / Text of Element:hover
        fg[SELECTED]      = "#eaeaea" # White / Main Headers in Menu 
        fg[INSENSITIVE]   = "#000000" # Black / Unselectable Menu Text
        fg[ACTIVE]        = "#777777" # Light-Grey / Unselected Layers name, Selected Check-Box

        text[NORMAL]      = "#b7b7b7" # White / Layers Name - no:hover state,number value in bars
        text[PRELIGHT]    = "#b7b7b7"
        text[SELECTED]    = "#b7b7b7" # Active Element Text, Color of Highlight Text
        text[INSENSITIVE] = "#000000" # Black Border for Widget
        text[ACTIVE]      = "#b7b7b7" #  Highlited but un-clicked Text

        base[NORMAL]      = "#434343" # Dark-Grey / Box BG, with value/property 
        base[PRELIGHT]    = "#434343"
        base[SELECTED]    = "#585858" # Dark-Grey / Highlight of Selected Element 
        base[INSENSITIVE] = "#3c3c3c" # Middle-Grey / Unselectable Window, empty or not for change
        base[ACTIVE]      = "#585858" # Dark-Grey / Highlight


 NautilusIconContainer::highlight_alpha=255
 NautilusIconContainer::dark_info_color = "#b7b7b7" # ?? RED
 NautilusIconContainer::light_info_color = "#b7b7b7" # ?? RED
