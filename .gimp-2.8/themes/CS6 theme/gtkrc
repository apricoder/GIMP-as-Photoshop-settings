style "StatusBar" = "rgb4u-default" {
       
        fg[NORMAL]        = "#777777" # Basic Status-Bar Text color
        
        text[NORMAL]      = "#777777" # % Value 
        text[PRELIGHT]    = "#b7b7b7" # PX:hover 
        text[SELECTED]    = "#b7b7b7" # Highlited Text 
        text[INSENSITIVE] = "#000000" # Unknow Black
        text[ACTIVE]      = "#b7b7b7" # Unknow

        base[NORMAL]      = "#3c3c3c" # Top and Bottom Borders for % value
        base[PRELIGHT]    = "#3c3c3c" # Unknow
        base[SELECTED]    = "#585858" # Highlited Text BG
        base[INSENSITIVE] = "#000000" # Unknow Black
        base[ACTIVE]      = "#585858" # Highlited Text BG and unclicked window
        
}
widget "*GimpStatusbar.*" style "StatusBar"

