########################################################
# Paned Widgets: Windows divided into two or more sections, which can be
# resized by dragging HANDLES

style "rgb4u-paned" = "rgb4u-default"{

  xthickness = 0
  ythickness = 0

  GtkPaned::handle_size = 2

engine "pixmap"{

# The handle on horizontal dividers :
image{
	function = HANDLE
	recolorable = TRUE
	
# You can change this to "IMG/Colors/transparent.png" or different file :
		
	file            = "IMG/Colors/2c2c2c.png" 
	border          = {0,0,0,0}
	stretch         = TRUE
	overlay_border = {0,0,0,0}
	overlay_stretch = FALSE
	orientation = HORIZONTAL
}

# The handle on vertical dividers :
image{
	function = HANDLE
	recolorable = TRUE
	
# You can change this to "IMG/Colors/transparent.png" or different file :
		
  	file            = "IMG/Colors/2c2c2c.png"
	border          = { 0,0,0,0 }
	stretch         = TRUE
	overlay_border = {0,0,0,0}
	overlay_stretch = FALSE
	orientation = VERTICAL
}
}

}

