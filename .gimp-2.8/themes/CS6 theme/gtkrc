########################################################
# GtkButton
########################################################
# You can create your own shadow effect for buttons by creating files :
# 70px x 25px 72ppi and replacing the current file path( vvv "IMG/")
#
style "rgb4u-button" {
  xthickness = 4
  ythickness = 2

engine "pixmap" {

image {
	function = BOX
	recolorable = TRUE
	state = NORMAL
	detail = "buttondefault"
	shadow = IN
	file = "IMG/Buttons/button_normal.png" # Button Normal
	border = {6,6,6,6}
	stretch = TRUE
}

image {
	function = BOX
	recolorable = TRUE
	state = NORMAL
	shadow = OUT
	file = "IMG/Buttons/button_normal.png" # Button Normal
	border = {6,6,6,6}
	stretch = TRUE
}

image {
	function = BOX
	recolorable     = TRUE
	state           = PRELIGHT
	shadow          = IN
	file            = "IMG/Buttons/button_hover.png" # Button Prelight
	border          = {6,6,6,6}
	stretch         = TRUE
}

image {
	function = BOX
	recolorable     = TRUE
	state           = ACTIVE
	shadow          = IN
	file            = "IMG/Buttons/button_hover.png" # Button Active
	border          = {6,6,6,6}
	stretch         = TRUE
}

image {

    function        = BOX
	recolorable     = TRUE
	state           = PRELIGHT
	shadow          = OUT
	file            = "IMG/Buttons/button_hover.png" # Button Prelight
	border          = {6,6,6,6}
	stretch         = TRUE
}
      
image {
    function        = BOX
	recolorable     = TRUE
	state           = NORMAL
	shadow          = IN
	file            = "IMG/Buttons/button_flat.png" # Button Flat
	border          = {6,6,6,6}
	stretch         = TRUE
}

image {
    function        = BOX
	recolorable     = TRUE
	state           = INSENSITIVE
	shadow          = IN
	file            = "IMG/Buttons/button_flat.png" # Button Flat
	border          = {6,6,6,6}
	stretch         = TRUE
}

image {
     function        = BOX
	recolorable     = TRUE
	state           = INSENSITIVE
	shadow          = OUT
	file            = "IMG/Buttons/button_flat.png" # Button Flat
	border          = {6,6,6,6}
	stretch         = TRUE
}

image {
    function        = BOX
	recolorable     = TRUE
	state           = SELECTED
	shadow          = IN
	file            = "IMG/Buttons/button_hover.png" # Button Push
	border          = {6,6,6,6}
	stretch         = TRUE
}

image  {
    function        = BOX
	recolorable     = TRUE
	state           = ACTIVE
	shadow          = IN
	file            = "IMG/Buttons/button_hover.png" # Button Active
	border          = {6,6,6,6}
	stretch         = TRUE
}

}

}
