		overlay_stretch = TRUE
      	}

   	image 
      	{
       		function        = SHADOW
 		recolorable     = TRUE
		shadow          = IN
		file            = "IMG/Colors/transparent.png" # Outside windows border - layers name, meassurements
		border          = {0,0,0,0}
		stretch         = TRUE
      	}	

        image 
      	{
       		function        = SHADOW
 		recolorable     = TRUE
		shadow          = ETCHED_IN
		file            = "IMG/Colors/transparent.png" # Unknow
		border          = {0,0,0,0}
		stretch         = TRUE
      	}	
      	
